@model Clinic.Models.Admin
@{
    ViewData["Title"] = "Create";

}
<style>
body, html {
            height: 100%;
        }
</style>
   

<link rel="stylesheet" href="~/css/window.css" />
<div class="window-wrapper">
    <div class="window" style="width:500px;">
        <div class="header">
            Profile
            <a href="/home" class="btn-X">X</a>
        </div>
        <div class="body">
            <br />
            <div class="row">
                <div class="col-lg-7">
                    <div>First Name: @Model.FirstName </div>
                    <div>Middle Name:   @Model.MiddleName</div>
                    <div>Last Name: @Model.LastName</div>
                </div>
                <div class="col-lg-5">

                </div>
                <div class="col-lg-7">
                    <div>Username: @Model.User.UserName</div>
                    <div>Email: @Model.User.Email</div>
                    <div>Phone: @Model.User.PhoneNumber</div>
                    <div>Mobile: @Model.Mobile</div>
                    <br />
                    <br />
                </div>
            </div>
        </div>
        <div class="footer">

            


        </div>

    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

