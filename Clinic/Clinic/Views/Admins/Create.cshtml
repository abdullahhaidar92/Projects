@model RegisterAdmin
@{
    ViewData["Title"] = "Register";
    
}


<html>
<body>
    <div class="window-wrapper">
        <div class="window">
            <div class="row">
                <div class="col-md-8">
                    <form asp-action="Create" method="post">
                        <div class="header">
                            <h1>Create Administrator Account.</h1>
                        </div>
                        <div class="body">


                            <div asp-validation-summary="All" class="text-danger"></div>
                            <div class="form-group">
                                <label asp-for="Username"></label><br />
                                <input asp-for="Username" class="form-control" value="abdullah" />
                                <span asp-validation-for="Username" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Email"></label><br />
                                <input asp-for="Email" class="form-control" value="abdullah2@web.com" />
                                <span asp-validation-for="Email" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Password"></label><br />
                                <input asp-for="Password" class="form-control" value="Aa12345$" />
                                <span asp-validation-for="Password" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="ConfirmPassword"></label><br />
                                <input asp-for="ConfirmPassword" class="form-control" value="Aa12345$" />
                                <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="FirstName"></label><br />
                                <input asp-for="FirstName" class="form-control" value="Abdullah" />
                                <span asp-validation-for="FirstName" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="MiddleName"></label><br />
                                <input asp-for="MiddleName" class="form-control" value="Hadi" />
                                <span asp-validation-for="MiddleName" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="LastName"></label><br />
                                <input asp-for="LastName" class="form-control" value="Haidar" />
                                <span asp-validation-for="LastName" class="text-danger"></span>
                            </div>

                            <div class="form-group">
                                <label asp-for="Phone"></label><br />
                                <input asp-for="Phone" class="form-control" value="(541) 754-3010" />
                                <span asp-validation-for="Phone" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Mobile"></label><br />
                                <input asp-for="Mobile" class="form-control" value="(541) 754-3010" />
                                <span asp-validation-for="Mobile" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="footer">
                            <button type="submit" class="btn btn-success" style="font-size:20px;">Register</button>
                        </div>

                    </form>
                </div>
            </div>
        </div>
        @section Scripts {
            <partial name="_ValidationScriptsPartial" />
        }
    </div>

</body>
</html>